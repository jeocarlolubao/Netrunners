<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="390dp"
    android:layout_height="250dp"
    android:background="#1E1E1E"
    tools:context=".HomeScreenFragment_3">

    <Button
        android:id="@+id/button_externalStorage"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginEnd="36dp"
        android:layout_marginRight="36dp"
        android:background="@drawable/roundcorners2"
        android:text="Extenal Storage Device"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/button_chair"
        app:layout_constraintEnd_toStartOf="@+id/button_chair"
        app:layout_constraintTop_toTopOf="@+id/button_chair"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/button_chair"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/roundcorners2"
        android:text="Computer Chairs"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.067" />

    <Button
        android:id="@+id/button_computerDesks"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="36dp"
        android:layout_marginLeft="36dp"
        android:background="@drawable/roundcorners2"
        android:text="Computer Desks"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/button_chair"
        app:layout_constraintStart_toEndOf="@+id/button_chair"
        app:layout_constraintTop_toTopOf="@+id/button_chair"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/button_cables"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginEnd="36dp"
        android:layout_marginRight="36dp"
        android:background="@drawable/roundcorners2"
        android:text="Cables"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/button_projector"
        app:layout_constraintEnd_toStartOf="@+id/button_projector"
        app:layout_constraintTop_toTopOf="@+id/button_projector"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/button_projector"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/roundcorners2"
        android:text="Projectors"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_chair"
        app:layout_constraintVertical_bias="0.244" />

    <Button
        android:id="@+id/button_router"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="36dp"
        android:layout_marginLeft="36dp"
        android:background="@drawable/roundcorners2"
        android:text="Routers"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/button_projector"
        app:layout_constraintStart_toEndOf="@+id/button_projector"
        app:layout_constraintTop_toTopOf="@+id/button_projector"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/button_1"
        android:layout_width="15dip"
        android:layout_height="15dip"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="20dp"
        android:background="@drawable/roundcorners"
        app:layout_constraintBottom_toBottomOf="@+id/button_2"
        app:layout_constraintEnd_toStartOf="@+id/button_2"
        app:layout_constraintTop_toTopOf="@+id/button_2"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/button_2"
        android:layout_width="15dip"
        android:layout_height="15dip"
        android:background="@drawable/roundcorners"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_projector"
        app:layout_constraintVertical_bias="0.306" />

    <Button
        android:id="@+id/button_3"
        android:layout_width="15dip"
        android:layout_height="15dip"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:background="@drawable/yellowroundcorners"
        app:layout_constraintBottom_toBottomOf="@+id/button_2"
        app:layout_constraintStart_toEndOf="@+id/button_2"
        app:layout_constraintTop_toTopOf="@+id/button_2"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView_prev"
        android:layout_width="25dp"
        android:layout_height="50dp"
        android:layout_marginTop="75dp"
        android:src="@drawable/arrow_left"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>